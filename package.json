{
    "name": "mudlet-map-renderer",
    "version": "0.0.1",
    "license": "MIT",
    "dependencies": {
        "babelify": "^10.0.0",
        "browserify": "^17.0.0",
        "paper-jsdom": "^0.12.15",
        "sharp": "^0.29.0",
        "tsc": "^2.0.3",
        "watchify": "^4.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/preset-env": "^6.15.0",
        "nodemon": "^2.0.7"
    },
    "browserify": {
        "transform": [
            [
                "babelify",
                {
                    "presets": [
                        [
                          "@babel/preset-env",
                          {
                            "useBuiltIns": "entry",
                            "corejs": "2"
                          }
                        ]
                      ]
                }
            ]
        ]
    },
    "scripts": {
        "build": "browserify index.js -o dist/bundle.js --exclude ./data/mapExport",
        "watch": "watchify index.js -o dist/bundle.js -v",
        "backend": "node backend.js",
        "dev": "nodemon -x \"yarn run backend\""
    },
    "exports": "./exports.js"
}
